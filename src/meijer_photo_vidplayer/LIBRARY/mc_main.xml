              <Actionscript>
                <script><![CDATA[import fl.video.FLVPlayback;
import fl.video.VideoEvent;
import flash.events.MouseEvent;
import flash.net.URLRequest;



var vp:FLVPlayback = mc_video.vidPlayer;
addVideoListeners();
vp.source = "http://c3323853.r53.cf0.rackcdn.com/Meijer_Poster.flv";
vp.playWhenEnoughDownloaded();
vp.getVideoPlayer(vp.activeVideoPlayerIndex).smoothing = true;


function addVideoListeners():void 
{
	vp.addEventListener( VideoEvent.COMPLETE, onVideoComplete );
	vp.addEventListener( VideoEvent.STATE_CHANGE, onVideoEvent );
}

function onVideoEvent( e:VideoEvent ):void 
{
	trace(e, e.state, flash.utils.getTimer());
	switch (e.state) {
		case "loading":
			mc_video.loadingAnim.visible = true;
			mc_video.endframe.visible = false;
			break;
		case "playing":
			mc_video.loadingAnim.visible = false;
			mc_video.endframe.visible = false;
			break;
	}
}

function onVideoComplete( e:VideoEvent ):void 
{
	//show replay button
	mc_video.endframe.visible = true;
}

btn_close.addEventListener( MouseEvent.CLICK, onCloseClick );
function onCloseClick( e:MouseEvent ):void {
	ExternalInterface.call("closeVideoModal");
}

mc_video.endframe.btn_cta.addEventListener( MouseEvent.CLICK, onCTAClick );
function onCTAClick( e:MouseEvent ):void {
	navigateToURL(new URLRequest("http://www.meijer.com/content/content_leftnav_manual.jsp?pageName=meijer_photo"), "_top");
}

mc_video.endframe.btn_replay.addEventListener( MouseEvent.CLICK, onReplayClick );
function onReplayClick( e:MouseEvent ):void {
	vp.seek(0);
	vp.play();
}]]></script>
